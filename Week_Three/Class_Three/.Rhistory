basic.grad.descent <- function(x, h=.0001){
max.iter <- 10000
stop.deriv <- 1e-10
derivative.step <- h
step.scale <- .5
iter <- 0
deriv <- Inf
beta <- 0.15
sse <- function(b){ sum((response - beta_0*predictor^b))}
for(i in 1:max.iter){
iter <- iter +1
}
deriv <-(sse(beta+deriv.step) -sse(beta))/deriv.step
beta <- beta -step.scale*deriv
if(abs(deriv)<stop.derv){break()}
fit <-list(beta=beta, iteration = iter, converged = (iter<max.iter))
return(x)
return(fit)
}
